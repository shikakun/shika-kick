{
  "name": "shika-kick",
  "version": "2.0.0",
  "config": {
    "distDir": "public",
    "srcDir": "src"
  },
  "scripts": {
    "build": "parcel build $npm_package_config_srcDir/index.html -d $npm_package_config_distDir",
    "clean": "rm -r $npm_package_config_distDir",
    "prebuild": "npm run clean",
    "start": "parcel $npm_package_config_srcDir/index.html -d $npm_package_config_distDir"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "normalize-scss": "^7.0.1",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.5.0",
    "sass": "^1.26.2"
  }
}
